<?xml version="1.0" encoding="UTF-8"?>
<widgetstyle>
    <title xml:lang="ko">Sininen Simple Strong</title>
    <description xml:lang="ko">심플 스트롱 위젯스타일을 기반으로 제작한, Sininen 스킨과 조화되는 위젯스타일.</description>
    <version>1.0</version>
    <date>2017-01-21</date>
    <preview>preview.png</preview>

    <author email_address="park_juwan@naver.com" link="http://parkjuwan.dothome.co.kr/">
        <name xml:lang="ko">Juwan Park</name>
    </author>

    <extra_vars>
        <var name="ws_colorset" type="select">
            <title xml:lang="ko">컬러셋</title>
            <description xml:lang="ko">원하는 컬러셋을 선택해주세요.</description>
            <options>
                <name xml:lang="ko">파란색</name>
				<value>blue</value>
            </options>
            <options>
                <name xml:lang="ko">제비꽃색</name>
				<value>violet</value>
            </options>
            <options>
                <name xml:lang="ko">연보라</name>
				<value>purple</value>
            </options>
            <options>
                <name xml:lang="ko">분홍색</name>
				<value>deeppink</value>
            </options>
            <options>
                <name xml:lang="ko">빨간색</name>
				<value>red</value>
            </options>
            <options>
                <name xml:lang="ko">주황색</name>
				<value>brown</value>
            </options>
            <options>
                <name xml:lang="ko">노란색</name>
				<value>yellow</value>
            </options>
            <options>
                <name xml:lang="ko">노랑연두</name>
				<value>gyellow</value>
            </options>
            <options>
                <name xml:lang="ko">초록색</name>
				<value>green</value>
            </options>
            <options>
                <name xml:lang="ko">에메랄드</name>
				<value>emerald</value>
            </options>
            <options>
                <name xml:lang="ko">청록색</name>
				<value>cyan</value>
            </options>
            <options>
                <name xml:lang="ko">하늘색</name>
				<value>skyblue</value>
            </options>
            <options>
                <name xml:lang="ko">회색</name>
				<value>gray</value>
            </options>
        </var>

        <var name="ws_title" type="text">
            <name xml:lang="ko">제목</name>
            <description xml:lang="ko">제목 텍스트</description>
        </var>
        <var name="ws_more_url" type="text">
            <name xml:lang="ko">더보기 URL</name>
            <description xml:lang="ko">http:// 포함한 URL를 등록합니다.</description>
        </var>
        <var name="ws_more_text" type="text">
            <name xml:lang="ko">더보기 텍스트</name>
            <description xml:lang="ko">더보기 텍스트</description>
       </var>
    </extra_vars>
</widgetstyle>
